<Window x:Class="client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        xmlns:view="clr-namespace:client.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Loaded ="MainWindow_Loaded">
    <Grid>
        <StackPanel>
            <Button HorizontalAlignment="Left" VerticalAlignment="Top" Content="Back" Margin="20 20" Click="ExitButton"/>
            <!--<view:AllDistrictsView x:Name = "AllDistrictsViewControl" Loaded = "AllDistrictsViewControl_Loaded"/>-->
            <ContentControl x:Name="MainContentControl"/>
        </StackPanel>
        
    </Grid>
</Window>
